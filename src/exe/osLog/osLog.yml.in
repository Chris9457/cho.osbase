service:
  name: "ILog"
  description: "api of the logs"
  realm: "osbase"
  namespace: "@OSBASE@.log"

process:
  - name: "getInputStream"
    type: "(async)string"
    description: "provide the input log stream"
    uri: "ILogService.getInputStream"

  - name: "getOutputStream"
    type: "(async)string"
    description: "provide the output log stream"
    uri: "ILogService.getOutputStream"
